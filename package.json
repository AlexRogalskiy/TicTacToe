{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "description": "Tic-Tac-Toe Node JS application",
  "private": true,
  "main": "start.js",
  "scripts": {
    "clean": "rimraf public",
    "serve": "node --require dotenv/config start.js",
    "prebuild": "npm run clean",
    "build": "webpack --config webpack.config.js --mode development --colors --progress --devtool sourcemap",
    "build-css": "node-sass src/sass/style.scss src/css/style.css",
    "build:umd": "webpack --output-filename index.umd.js",
    "build:umd.min": "webpack --output-filename index.umd.min.js -p",
    "lint": "node node_modules/eslint/bin/eslint --ext .js --config=.eslintrc.json src/js/libs",
    "watch": "webpack --watch --config webpack.config.js --mode development",
    "dev": "SET ENV_SRC='./.env.development.local' && nodemon start.js",
    "build-dev": "set NODE_ENV=development && npm run build-css && npm run build",
    "build-prod": "set NODE_ENV=production && npm run build-css && npm run build",
    "start": "webpack-dev-server --hot --mode development --config webpack.config.js --progress --colors --watch --inline --open",
    "test": "mocha --ui tdd --reporter spec --recursive --require babel-polyfill --watch tests/*.test.js",
    "test:watch": "jest --watch",
    "test:libs": "node --require dotenv/config ./tests/node-suite.js",
    "test:debug": "mocha debug -R spec --require babel-polyfill ./tests/**/*.js",
    "test:coverage": "jest --coverage",
    "publish": "npm publish",
    "prettify": "prettier --list-different --write src/js/**/*.{js,json,md}",
    "flow": "flow",
    "c:start": "concurrently \"npm run c:server\" \"npm run dev\"",
    "c:build": "react-scripts build",
    "c:test": "react-scripts test --env=jsdom",
    "c:eject": "react-scripts eject",
    "c:server": "pouchdb-server --host localhost -p 10102 -d ./db",
    "c:http": "serve -p 3000 -s public/build",
    "c:offline": "concurrently \"npm run c:server\" \"npm run c:http\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexRogalskiy/tic-tac-toe"
  },
  "keywords": [
    "Tic-Tac-Toe",
    "Node.js",
    "Online multiplayer game",
    "React UI"
  ],
  "author": "Alexander R <alexander.rogalsky@yandex.ru> (https://linkedin.com/in/alexander-rogalsky-985a4828)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlexRogalskiy/tic-tac-toe/issues"
  },
  "homepage": "https://github.com/AlexRogalskiy/tic-tac-toe#readme",
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
