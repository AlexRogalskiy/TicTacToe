    "npm": "^5.0.0"
  },
  "sideEffects": "false",
  "browserslist": [
    "last 2 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "workspaces": [
    "src/*"
  ],
  "config": {
    "ghooks": {
      "pre-commit": "npm run test"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "/tests/.*\\.test\\.js$",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/vendor/*.js"
    ]
  }
