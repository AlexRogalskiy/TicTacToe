{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "description": "Tic-Tac-Toe Node JS application",
  "private": true,
  "main": "server.js",
  "scripts": {
    "serve": "node server.js",
    "build": "webpack --config webpack.config.js",
    "build:umd": "webpack --output-filename index.umd.js",
    "build:umd.min": "webpack --output-filename index.umd.min.js -p",
    "watch": "webpack --watch --config webpack.config.js",
    "build-dev": "set NODE_ENV=development && npm run build",
    "build-prod": "set NODE_ENV=production && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexRogalskiy/tic-tac-toe"
  },
  "keywords": [
    "Tic-Tac-Toe",
    "Node.js",
    "Online multiplayer game",
    "React UI"
  ],
  "author": "Alexander R <alexander.rogalsky@yandex.ru> (https://linkedin.com/in/alexander-rogalsky-985a4828)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlexRogalskiy/tic-tac-toe/issues"
  },
  "homepage": "https://github.com/AlexRogalskiy/tic-tac-toe#readme",
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2016": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "copy-webpack-plugin": "4.5.2",
    "css-loader": "1.0.0",
    "dateformat": "3.0.2",
    "html-webpack-plugin": "3.2.0",
    "json-loader": "0.5.7",
    "sass-loader": "7.1.0",
    "serve-static": "1.13.2",
    "style-loader": "0.23.0",
    "typestyle": "1.3.7",
    "webpack": "4.19.1",
    "webpack-cli": "3.1.0",
    "webpack-config": "7.5.0"
  },
  "dependencies": {
    "body-parser": "1.18.3",
    "connect": "3.6.6",
    "express": "4.16.3",
    "react": "16.5.2",
    "react-addons-update": "15.6.2",
    "react-dom": "16.5.2",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-validation-mixin": "5.4.0",
    "react-validatorjs-strategy": "0.2.1",
    "redux": "4.0.0",
    "redux-logger": "3.0.6"
  },
  "engines": {
    "node": "0.12.x"
  }
}
